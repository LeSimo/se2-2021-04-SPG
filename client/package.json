{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@craco/craco": "^6.4.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "dayjs": "^1.10.7",
    "framer-motion": "^4.1.17",
    "history": "^5.1.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-html-reporter": "^3.4.2",
    "jest-junit": "^13.0.0",
    "mockdate": "^3.0.5",
    "plotly.js": "^2.8.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-datepicker": "^4.5.0",
    "react-dom": "^17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-icons": "^4.3.1",
    "react-plotly.js": "^2.5.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^5.2.1",
    "web-vitals": "^1.0.1",
    "xunit-viewer": "^7.1.5"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "testCov": "craco test --coverage --watchAll",
    "testJson": "craco test --watchAll --json --outputFile=coverage/test-summary.json",
    "testHtml": "craco test --watchAll --reporters=jest-html-reporter",
    "testJunit": "craco test --watchAll --reporters=jest-junit",
    "viewJunit": "npx xunit-viewer -r junit.xml -s",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "react/no-multi-comp": "error",
      "react/jsx-pascal-case": "error",
      "react/prop-types": "off",
      "no-nested-ternary": "off",
      "no-console": [
        "warn",
        {
          "allow": [
            "error",
            "info"
          ]
        }
      ],
      "react/jsx-indent": "off",
      "react/jsx-wrap-multilines": [
        "error",
        {
          "declaration": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "generated-types.js"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/reportWebVitals.js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "mockdate": "^3.0.5",
    "postcss": "^7.0.39",
    "prettier": "2.4.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  },
  "proxy": "http://localhost:3001/"
}
